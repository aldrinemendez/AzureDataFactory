{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidrepotrailproject"
		},
		"ls_AzureDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorageGen2'"
		},
		"ls_ablob_covidrepotrailsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidrepotrailsa'"
		},
		"ls_adls_covidreportraildl_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adls_covidreportraildl'"
		},
		"ls_config_covid_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_config_covid'"
		},
		"ls_AzureDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidrepotraildl.dfs.core.windows.net/"
		},
		"ls_Http_ecdc_opendata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidrepotrailproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLakeStorageGen2_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorageGen2_accountKey')]"
					},
					"ls_ablob_covidrepotrailsa_connectionString": {
						"value": "[parameters('ls_ablob_covidrepotrailsa_connectionString')]"
					},
					"ls_adls_covidreportraildl_connectionString": {
						"value": "[parameters('ls_adls_covidreportraildl_connectionString')]"
					},
					"ls_config_covid_connectionString": {
						"value": "[parameters('ls_config_covid_connectionString')]"
					},
					"ls_AzureDataLakeStorageGen2_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorageGen2_properties_typeProperties_url')]"
					},
					"ls_Http_ecdc_opendata_properties_typeProperties_url": {
						"value": "[parameters('ls_Http_ecdc_opendata_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covidrepotrailproject_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covidrepotrailproject_ArmTemplate_0')]"
			]
		}
	]
}